<?xml version="1.0"?>
<project name="Javascript compress project" basedir="." default="compile">
 
    <taskdef name="jscomp" classname="com.google.javascript.jscomp.ant.CompileTask" 
    	classpath="${basedir}/../../../tools/ant/compiler.jar"/>
    <target name="compile">
        <jscomp compilationLevel="simple" warning="quiet" debug="false" output="${basedir}/../TilesPuzzleHtml5-v1.0.js">
            <sources dir="${basedir}">
				<file name="GridLayer.js" />
				<file name="GameRuleLayer.js" />
				<file name="LeaderboardLayer.js" />
				<file name="MainMenuLayer.js" />
				<file name="GameLayer.js" />
				<file name="PauseMenuLayer.js" />
				<file name="resource.js" />
				<file name="ScoreObject.js" />
				<file name="SubmitScoreLayer.js" />
				<file name="TileController.js" />
				<file name="TileLayer.js" />
				<file name="TileManager.js" />
				<file name="TileSprite.js" />
            	<file name="constant.js" />
				<file name="Helper.js" />
				<file name="ShapeSprite.js" />
				<file name="LanguageUtil.js" />
            </sources>
        </jscomp>
     </target>
 </project>